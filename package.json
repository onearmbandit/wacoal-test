{
  "name": "wp-headless-react-ssr",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=development node ./webpack/start.js",
    "build": "NODE_ENV=production node ./webpack/build.js",
    "sniff:setup" : "chmod +x sniff_setup.sh && ./sniff_setup.sh",
    "docker:start": "npm run photon:setup && docker-compose down && docker-compose up --build",
    "docker:setup": "docker exec -it wacoal-php sh -c /setup.sh",
    "docker:shell": "docker exec -it wacoal-php /bin/bash",
    "docker:ip": "docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' wacoal-nginx",
    "photon:setup": "unzip -o docker/photon_local.zip",
    "phpcs": "phpcs --standard=WordPress-VIP-Go -sp --basepath=. --ignore=node_modules/* theme*"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "exports-loader": "^0.7.0",
    "imports-loader": "^0.8.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "precss": "^4.0.0",
    "tether": "^1.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^3.1.0",
    "dotenv": "^8.0.0",
    "file-loader": "^4.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.37.0",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
