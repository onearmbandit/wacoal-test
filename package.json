{
  "name": "wacoal",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=development NODE_SITE=wacoal node ./webpack/start.js",
    "build": "NODE_ENV=production NODE_SITE=wacoal node ./webpack/build.js",
    "btemptd-start": "NODE_ENV=development NODE_SITE=btemptd node ./webpack/start.js",
    "btemptd-build": "NODE_ENV=production NODE_SITE=btemptd node ./webpack/build.js",
    "sniff:setup": "chmod +x sniff_setup.sh && ./sniff_setup.sh",
    "docker:start": "npm run photon:setup && docker-compose down && COMPOSE_HTTP_TIMEOUT=200 docker-compose up --build",
    "docker:setup": "docker exec -it wacoal-php sh -c /setup.sh",
    "docker:shell": "docker exec -it wacoal-php /bin/bash",
    "docker:ip": "docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' wacoal-nginx",
    "photon:setup": "unzip -o docker/photon_local.zip",
    "phpcs": "phpcs --standard=WordPress-VIP-Go -sp --basepath=. --ignore=themes/wacoal/dist* themes/wacoal*",
    "btemptd-phpcs": "phpcs --standard=WordPress-VIP-Go -sp --basepath=. --ignore=themes/btemptd/dist* themes/btemptd*"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@wordpress/babel-plugin-makepot": "^2.1.1",
    "@wordpress/babel-preset-default": "^3.0.0",
    "@wordpress/browserslist-config": "^2.1.3",
    "@wordpress/i18n": "^3.0.0",
    "ajv": "^6.5.4",
    "assets-webpack-plugin": "^3.9.12",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "bootstrap": "^4.3.1",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^1.0.0",
    "core-js": "^2.6.10",
    "cross-env": "^5.0.1",
    "css-loader": "^3.1.0",
    "dompurify": "^2.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.6.1",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-wordpress": "^0.1.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "import-glob": "^1.5.0",
    "imports-loader": "^0.8.0",
    "jquery": "^3.4.1",
    "jquery-mousewheel": "^3.1.13",
    "lazysizes": "^5.2.0",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "node-sass-glob-importer": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "popper.js": "^1.16.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "select2": "^4.0.12",
    "stats-webpack-plugin": "^0.7.0",
    "stickyfilljs": "^2.1.0",
    "style-loader": "^0.23.1",
    "swiper": "^5.4.5",
    "terser-webpack-plugin": "^1.3.0",
    "tether": "^1.4.7",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.1.1"
  },
  "devDependencies": {
